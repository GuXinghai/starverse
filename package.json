{
  "name": "starverse",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "chcp 65001 >nul 2>&1 & cross-env FORCE_COLOR=1 vite --host",
    "build": "vue-tsc && vite build && electron-builder",
    "preview": "vite preview",
    "electron:dev": "chcp 65001 >nul 2>&1 & cross-env FORCE_COLOR=1 concurrently --kill-others \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "npm run build",
    "dev:clean": "chcp 65001 >nul 2>&1 & taskkill /F /IM electron.exe 2>nul & npm run dev",
    "dev:quiet": "chcp 65001 >nul 2>&1 & cross-env FORCE_COLOR=1 NODE_ENV=development vite --host --logLevel silent"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "electron-store": "^11.0.2",
    "pinia": "^3.0.3",
    "uuid": "^13.0.0",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^30.0.1",
    "electron-builder": "^24.13.3",
    "electron-vite": "^4.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vite-plugin-electron": "^0.28.6",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue-tsc": "^2.0.26",
    "wait-on": "^9.0.1"
  },
  "main": "dist-electron/main.js"
}
